{
  "name": "dotnet-app",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "dotnet build apps/dotnet-app"
      }
    },
    "run": {
      "executor": "nx:run-commands",
      "options": {
        "command": "dotnet run --project apps/dotnet-app"
      }
    }
  }
}
